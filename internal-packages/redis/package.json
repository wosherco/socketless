{
  "name": "@socketless/redis",
  "version": "0.1.2",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./dist/src/client.d.ts",
      "default": "./src/client.ts"
    },
    "./schemas": {
      "types": "./dist/src/schemas.d.ts",
      "default": "./src/schemas.ts"
    },
    "./env": "./env.ts"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "push": "pnpm with-env drizzle-kit push",
    "generate": "pnpm with-env drizzle-kit generate",
    "migrate": "pnpm with-env drizzle-kit migrate",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@socketless/shared": "workspace:*",
    "@socketless/validators": "workspace:*",
    "@t3-oss/env-nextjs": "0.11.0",
    "ioredis": "^5.4.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@socketless/eslint-config": "workspace:*",
    "@socketless/prettier-config": "workspace:*",
    "@socketless/tsconfig": "workspace:*",
    "dotenv-cli": "7.4.2",
    "eslint": "9.9.1",
    "prettier": "3.3.3",
    "typescript": "5.5.4"
  },
  "prettier": "@socketless/prettier-config"
}
