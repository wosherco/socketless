{
  "name": "@socketless/docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "pnpm with-env docusaurus start",
    "dev": "pnpm with-env docusaurus start -p 3002",
    "build:cf": "pnpm with-env docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "clean": "git clean -xdf .cache .next .turbo node_modules build .docusaurus",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@docusaurus/core": "3.5.2",
    "@docusaurus/preset-classic": "3.5.2",
    "@mdx-js/react": "^3.0.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "redocusaurus": "^2.1.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.5.2",
    "@docusaurus/tsconfig": "3.5.2",
    "@docusaurus/types": "3.5.2",
    "@socketless/eslint-config": "workspace:*",
    "@socketless/prettier-config": "workspace:*",
    "@socketless/tailwind-config": "workspace:*",
    "@socketless/tsconfig": "workspace:*",
    "dotenv-cli": "7.4.2",
    "eslint": "9.9.1",
    "prettier": "3.3.3",
    "typescript": "5.5.4",
    "postcss": "8.4.41",
    "tailwindcss": "3.4.10"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "prettier": "@socketless/prettier-config"
}
