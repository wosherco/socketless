{
  "name": "@socketless/docs",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun with-env astro dev",
    "start": "bun with-env astro dev",
    "build": "bun with-env astro build",
    "build:cf": "astro build",
    "preview": "bun with-env astro preview",
    "astro": "astro",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path .prettierignore",
    "lint": "eslint && astro check",
    "typecheck": "tsc --noEmit",
    "clean": "git clean -xdf .next .turbo node_modules",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@astrojs/check": "0.9.3",
    "@astrojs/starlight": "0.26.1",
    "astro": "4.10.2",
    "sharp": "0.33.5"
  },
  "devDependencies": {
    "@socketless/eslint-config": "workspace:*",
    "@socketless/prettier-config": "workspace:*",
    "@socketless/tailwind-config": "workspace:*",
    "@socketless/tsconfig": "workspace:*",
    "@types/node": "20.14.15",
    "eslint": "9.9.1",
    "prettier": "3.3.3",
    "typescript": "5.5.4"
  },
  "prettier": "@socketless/prettier-config"
}
