{
  "name": "@socketless/nextjs",
  "version": "0.0.1",
  "type": "module",
  "private": false,
  "homepage": "https://socketless.ws",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "react-server": "./dist/index.react-server.js",
      "default": "./dist/index.js"
    }
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "bunchee --tsconfig tsconfig.json",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "bunchee -w --tsconfig tsconfig.json --no-clean",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "prepack": "bun ../../.github/replace-workspace-protocol.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "peerDependencies": {
    "next": "*",
    "react": "^17.0.2 || ^18.0.0",
    "socketless.ws": "^1.5.1"
  },
  "dependencies": {
    "@socketless/react": "workspace:*",
    "@socketless/shared": "workspace:*",
    "server-only": "^0.0.1",
    "socketless.ws": "workspace:*",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@socketless/eslint-config": "workspace:*",
    "@socketless/prettier-config": "workspace:*",
    "@socketless/tsconfig": "workspace:*",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "9.9.1",
    "next": "14.2.12",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "typescript": "5.5.4"
  },
  "prettier": "@socketless/prettier-config",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist"
  ]
}
